{
  "name": "fridge-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",

    "fix:lint": "eslint . --fix || exit 0",
    "fix:format": "prettier --write . || exit 0",
    "check:types": "tsc --noEmit || exit 0",

    "msg": "node -e \"const d=new Date();console.log(`${d.getFullYear()}-${String(d.getMonth()+1).padStart(2,'0')}-${String(d.getDate()).padStart(2,'0')} ${String(d.getHours()).padStart(2,'0')}:${String(d.getMinutes()).padStart(2,'0')}:${String(d.getSeconds()).padStart(2,'0')}`)\"",

    "push": "npm run fix:lint && npm run fix:format && npm run check:types && git add -A && git commit -m \"update: $(npm run -s msg)\" && git push"
  },

  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@simplewebauthn/server": "^13.2.2",
    "@zxing/browser": "^0.1.5",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^5.22.0",
    "prisma": "^6.16.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.0",
    "react-mobile-datepicker": "^4.0.2",
    "sonner": "^2.0.7",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1"
  },

  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/stripe": "^8.0.416",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}

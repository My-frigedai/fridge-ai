{
  "name": "fridge-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "fix:lint": "eslint . --fix || exit 0",
    "fix:format": "prettier --write . || exit 0",
    "check:types": "tsc --noEmit || exit 0",
    "msg": "node -e \"const d=new Date();console.log(`${d.getFullYear()}-${String(d.getMonth()+1).padStart(2,'0')}-${String(d.getDate()).padStart(2,'0')} ${String(d.getHours()).padStart(2,'0')}:${String(d.getMinutes()).padStart(2,'0')}:${String(d.getSeconds()).padStart(2,'0')}`)\"",
    "push": "npm run fix:lint && npm run fix:format && npm run check:types && git add -A && git commit -m \"update: $(npm run -s msg)\" && git push"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.19.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@simplewebauthn/server": "^13.1.0",
    "@zxing/browser": "^0.0.9",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "framer-motion": "^11.11.17",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "next": "15.5.7",
    "next-auth": "4.24.7",
    "nodemailer": "^6.9.15",
    "openai": "^5.23.0",
    "prisma": "^5.19.1",
    "qrcode": "^1.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-datepicker": "^6.7.0",
    "react-mobile-datepicker": "^4.0.2",
    "sonner": "^1.5.0",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.4",
    "@types/stripe": "^8.0.416",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.7",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.7.2"
  }
}
